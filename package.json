{
  "name": "advent-of-code",
  "version": "1.0.0",
  "description": "Advent of Code",
  "scripts": {
    "start": "ts-node $1",
    "lint": "eslint . --color",
    "prettier": "prettier --check .",
    "watch": "tsc -w",
    "prepare": "husky"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/compat": "1.2.3",
    "@eslint/js": "9.15.0",
    "@types/node": "20.8.2",
    "esbuild": "0.19.9",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "prettier": "3.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.16.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown .",
    "*.ts": "eslint --color --fix --max-warnings=0"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "10.2.5"
  }
}
